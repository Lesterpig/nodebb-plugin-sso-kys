{
	"id": "nodebb-plugin-sso-kys",
	"name": "NodeBB Keep Your Soul SSO",
	"description": "NodeBB Plugin that allows users to login/register via Keep Your Soul.",
	"url": "https://github.com/Lesterpig/nodebb-plugin-sso-kys",
	"library": "./library.js",
	"hooks": [
		{ "hook": "filter:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	],
	"minver": "0.5.0"
}
